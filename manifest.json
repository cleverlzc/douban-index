{
  "manifest_version": 2,
  "name": "豆瓣首页",
  "author": "litten",
  "description": "展示豆瓣默认首页",
  "version": "1.1",
  "icons": {
    "128": "icon/128.png",
    "16": "icon/16.png",
    "48": "icon/48.png"
  },
  "content_scripts": [ {
    "js": ["content.js"],
    "matches": [ "*://*/*", "file:///*" ]
  } ],
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
		"<all_urls>",
		"tabs",
		"cookies",
		"webRequest",
		"webRequestBlocking",
		"management"
  ]
}